{
  "name": "krigsseilerregisteret-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run i18n && next build",
    "start": "next start",
    "lint": "next lint",
    "i18n:extract": "formatjs extract \"src/**/*.{js,ts,tsx}\"  --id-interpolation-pattern [sha512:contenthash:base64:6] --out-file src/lang/messages.json",
    "i18n:prepareTranslations": "node scripts/prepareTranslations.js",
    "i18n:compile": "formatjs compile-folder --ast  src/lang/locales src/lang/compiled-locales",
    "i18n": "npm run i18n:extract && npm run i18n:prepareTranslations && npm run i18n:compile",
    "rustywind-fix": "rustywind --write ."
  },
  "dependencies": {
    "@formatjs/intl-numberformat": "7.2.5",
    "@headlessui/react": "^1.4.3",
    "@reach/accordion": "0.16.1",
    "@reach/dialog": "0.16.2",
    "@reach/tabs": "^0.16.4",
    "@react-google-maps/api": "2.7.0",
    "@react-hook/debounce": "4.0.0",
    "@react-hook/media-query": "1.1.1",
    "@react-hook/resize-observer": "^1.2.4",
    "@tailwindcss/forms": "0.4.0",
    "@types/react": "17.0.31",
    "autoprefixer": "^10.4.0",
    "google-map-react": "2.1.10",
    "html-react-parser": "^1.4.0",
    "lodash.range": "3.2.0",
    "minisearch": "^6.0.0-beta.1",
    "next": "^12.3.4",
    "photoswipe": "^4.1.3",
    "postcss": "^8.3.11",
    "react": "17.0.2",
    "react-clamp-lines": "^3.0.3",
    "react-dom": "17.0.2",
    "react-intl": "5.20.13",
    "react-minisearch": "^6.0.0-beta.1",
    "react-photoswipe-gallery": "^1.3.6",
    "react-select": "^5.2.0",
    "swiper": "^9.3.2",
    "swr": "1.1.2-beta.0",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.4.0",
    "@next/eslint-plugin-next": "^11.1.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "babel-plugin-formatjs": "^10.3.11",
    "eslint": "^7.28.0",
    "eslint-config-next": "^12.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-formatjs": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "fs-extra": "10.0.0",
    "prettier": "^2.4.1",
    "rustywind": "^0.12.2",
    "typedoc": "^0.22.7",
    "typescript": "^4.4.4"
  }
}
